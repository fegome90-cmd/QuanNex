{
  "dashboard": {
    "id": null,
    "title": "QuanNex Metrics Integrity Gate - Dashboard Operador",
    "tags": ["quannex", "metrics", "operator", "blindaje"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Live vs Snapshot Ratio (%)",
        "type": "stat",
        "targets": [
          {
            "expr": "100 * (1 - rate(quannex_metrics_snapshot_total[5m]) / rate(quannex_metrics_total[5m]))",
            "legendFormat": "Live Ratio %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Fallback Count Rate (15m)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(quannex_metrics_fallback_total[15m])",
            "legendFormat": "Fallback Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Fallbacks/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "p95 Latency /metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(qn_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 Latency"
          }
        ],
        "yAxes": [
          {
            "label": "seconds",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "E2E Last Pass Age (minutes)",
        "type": "graph",
        "targets": [
          {
            "expr": "(time() - quannex_e2e_last_pass_timestamp) / 60",
            "legendFormat": "Minutes since last E2E PASS"
          }
        ],
        "yAxes": [
          {
            "label": "minutes",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Breaker Flips (eventos por día)",
        "type": "graph",
        "targets": [
          {
            "expr": "increase(quannex_circuit_breaker_active[1d])",
            "legendFormat": "Circuit Breaker Flips"
          }
        ],
        "yAxes": [
          {
            "label": "flips/day",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Snapshot Streak (máximo consecutivo en 24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "max_over_time(quannex_metrics_snapshot_total[1d])",
            "legendFormat": "Max Snapshot Streak"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8}
      },
      {
        "id": 7,
        "title": "Live Recovery Time (minutos desde 1er snapshot a 1er live)",
        "type": "graph",
        "targets": [
          {
            "expr": "time() - quannex_last_live_timestamp",
            "legendFormat": "Minutes since last live"
          }
        ],
        "yAxes": [
          {
            "label": "minutes",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8}
      },
      {
        "id": 8,
        "title": "Build Info & Version",
        "type": "table",
        "targets": [
          {
            "expr": "quannex_build_info",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "annotations": {
      "list": [
        {
          "name": "Circuit Breaker Active",
          "datasource": "Prometheus",
          "expr": "quannex_circuit_breaker_active == 1",
          "iconColor": "red",
          "title": "Circuit Breaker Activado"
        },
        {
          "name": "High Fallback Rate",
          "datasource": "Prometheus", 
          "expr": "rate(quannex_metrics_fallback_total[15m]) > 0.1",
          "iconColor": "yellow",
          "title": "Alta Tasa de Fallback"
        },
        {
          "name": "E2E Stale",
          "datasource": "Prometheus",
          "expr": "time() - quannex_e2e_last_pass_timestamp > 5400",
          "iconColor": "orange",
          "title": "E2E Obsoleto"
        },
        {
          "name": "Deployment",
          "datasource": "Prometheus",
          "expr": "changes(quannex_build_info[1h]) > 0",
          "iconColor": "blue",
          "title": "Nuevo Deployment"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(quannex_gate_status, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    }
  }
}
