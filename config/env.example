# QuanNex Environment Variables
# Copy to .env and fill with actual values

# Metrics & Monitoring
METRICS_URL=http://localhost:3000/metrics
METRICS_PORT=3000

# Workflow Configuration  
QN_WORKFLOW_CMD=npm
QN_WORKFLOW_ARGS=run verify

# AutoFix Configuration
AUTOFIX_MAX_RISK=3
AUTOFIX_BRANCH_PREFIX=autofix/

# MCP Server Configuration
MCP_SERVER_PORT=3001
MCP_SERVER_HOST=localhost

# Security (DO NOT commit real values)
# SECRET_KEY=your-secret-key-here
# API_TOKEN=your-api-token-here